//Palindrome or not
#include<stdio.h>
#include<conio.h>
int main()
{
	int num,rev=0,rem,temp;
	printf("enter the number");
	scanf("%d",&num);
	temp=num;
	while(num!=0)
    {
    	rem=num%10;//used to seperate a single number
    	rev=rev*10+rem;
    	num=num/10;
	}
	if(temp==rev)
	printf("Palindrome");
	else	
	printf("Not Palindrome");
}
